{
  "compilerOptions": {
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "target": "es2015",
    "module": "commonjs",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./js",
    "watch": true
  },
  "exclude": [
    "node_modules"
  ]
}
